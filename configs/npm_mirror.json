{
  "version": "1.0.0",
  "tools": [
    {
      "id": "npm",
      "name": "NPM",
      "description": "Node Package Manager",
      "detection": {
        "command": "npm",
        "arguments": [
          "--version"
        ]
      },
      "sources": [
        {
          "id": "npm-official",
          "name": "官方源",
          "url": "https://registry.npmjs.org/",
          "description": "npm 官方源"
        },
        {
          "id": "npm-taobao",
          "name": "淘宝镜像",
          "url": "https://registry.npmmirror.com/",
          "description": "淘宝镜像（新域名）",
          "region": "CN"
        },
        {
          "id": "npm-tencent",
          "name": "腾讯云",
          "url": "https://mirrors.cloud.tencent.com/npm/",
          "description": "腾讯云镜像",
          "region": "CN"
        },
        {
          "id": "npm-huawei",
          "name": "华为云",
          "url": "https://mirrors.huaweicloud.com/repository/npm/",
          "description": "华为云镜像",
          "region": "CN"
        },
        {
          "id": "npm-aliyun",
          "name": "阿里云",
          "url": "https://mirrors.aliyun.com/npm/",
          "description": "阿里云镜像",
          "region": "CN"
        },
        {
          "id": "npm-163",
          "name": "网易163",
          "url": "https://mirrors.163.com/npm/",
          "description": "网易163镜像",
          "region": "CN"
        },
        {
          "id": "npm-tsinghua",
          "name": "清华大学",
          "url": "https://mirrors.tuna.tsinghua.edu.cn/npm/",
          "description": "清华大学 TUNA 镜像",
          "region": "CN"
        },
        {
          "id": "npm-sjtu",
          "name": "上海交大",
          "url": "https://mirrors.sjtug.sjtu.edu.cn/npm/",
          "description": "上海交通大学 SJTUG 镜像",
          "region": "CN"
        },
        {
          "id": "npm-ustc",
          "name": "中科大",
          "url": "https://mirrors.ustc.edu.cn/npm/",
          "description": "中科大 USTC 镜像",
          "region": "CN"
        },
        {
          "id": "npm-zju",
          "name": "浙江大学",
          "url": "https://mirrors.zju.edu.cn/npm/",
          "description": "浙江大学 ZJU 镜像",
          "region": "CN"
        },
        {
          "id": "npm-bfsu",
          "name": "北外 BFSU",
          "url": "https://mirrors.bfsu.edu.cn/npm/",
          "description": "北京外国语大学 BFSU 镜像",
          "region": "CN"
        }
      ],
      "strategy": {
        "type": "command",
        "set": {
          "command": "npm",
          "arguments": [
            "config",
            "set",
            "registry",
            "{{url}}"
          ]
        },
        "get": {
          "command": "npm",
          "arguments": [
            "config",
            "get",
            "registry"
          ],
          "outputParser": "trim"
        }
      },
      "backup": {
        "filePath": "~/.npmrc",
        "backupFileName": ".npmrc.backup",
        "backupOriginal": true
      },
      "metadata": {
        "supportedPlatforms": [
          "macOS",
          "linux",
          "windows"
        ],
        "supportsSpeedTest": true,
        "documentationURL": "https://docs.npmjs.com/"
      }
    }
  ]
}
