{
  "version": "1.0.0",
  "tools": [
    {
      "id": "npm",
      "name": "NPM",
      "description": "Node Package Manager",
      "detection": {
        "command": "npm",
        "arguments": [
          "--version"
        ]
      },
      "sources": [
        {
          "id": "npm-official",
          "name": "官方源",
          "url": "https://registry.npmjs.org/",
          "description": "npm 官方源"
        },
        {
          "id": "npm-taobao",
          "name": "淘宝镜像",
          "url": "https://registry.npmmirror.com/",
          "description": "淘宝镜像（新域名）",
          "region": "CN"
        },
        {
          "id": "npm-tencent",
          "name": "腾讯云",
          "url": "https://mirrors.cloud.tencent.com/npm/",
          "description": "腾讯云镜像",
          "region": "CN"
        },
        {
          "id": "npm-huawei",
          "name": "华为云",
          "url": "https://mirrors.huaweicloud.com/repository/npm/",
          "description": "华为云镜像",
          "region": "CN"
        },
        {
          "id": "npm-aliyun",
          "name": "阿里云",
          "url": "https://mirrors.aliyun.com/npm/",
          "description": "阿里云镜像",
          "region": "CN"
        },
        {
          "id": "npm-163",
          "name": "网易163",
          "url": "https://mirrors.163.com/npm/",
          "description": "网易163镜像",
          "region": "CN"
        },
        {
          "id": "npm-tsinghua",
          "name": "清华大学",
          "url": "https://mirrors.tuna.tsinghua.edu.cn/npm/",
          "description": "清华大学 TUNA 镜像",
          "region": "CN"
        },
        {
          "id": "npm-sjtu",
          "name": "上海交大",
          "url": "https://mirrors.sjtug.sjtu.edu.cn/npm/",
          "description": "上海交通大学 SJTUG 镜像",
          "region": "CN"
        },
        {
          "id": "npm-ustc",
          "name": "中科大",
          "url": "https://mirrors.ustc.edu.cn/npm/",
          "description": "中科大 USTC 镜像",
          "region": "CN"
        },
        {
          "id": "npm-zju",
          "name": "浙江大学",
          "url": "https://mirrors.zju.edu.cn/npm/",
          "description": "浙江大学 ZJU 镜像",
          "region": "CN"
        },
        {
          "id": "npm-bfsu",
          "name": "北外 BFSU",
          "url": "https://mirrors.bfsu.edu.cn/npm/",
          "description": "北京外国语大学 BFSU 镜像",
          "region": "CN"
        }
      ],
      "strategy": {
        "type": "command",
        "set": {
          "command": "npm",
          "arguments": [
            "config",
            "set",
            "registry",
            "{{url}}"
          ]
        },
        "get": {
          "command": "npm",
          "arguments": [
            "config",
            "get",
            "registry"
          ],
          "outputParser": "trim"
        }
      },
      "backup": {
        "filePath": "~/.npmrc",
        "backupFileName": ".npmrc.backup",
        "backupOriginal": true
      },
      "metadata": {
        "supportedPlatforms": [
          "macOS",
          "linux",
          "windows"
        ],
        "supportsSpeedTest": true,
        "documentationURL": "https://docs.npmjs.com/"
      }
    },
    {
      "id": "maven",
      "name": "Maven",
      "description": "Apache Maven 项目管理工具",
      "detection": {
        "command": "mvn",
        "arguments": [
          "--version"
        ],
        "fallbackDetection": {
          "type": "file",
          "path": "~/.m2/settings.xml"
        }
      },
      "sources": [
        {
          "id": "maven-official",
          "name": "官方源",
          "url": "https://repo.maven.apache.org/maven2/",
          "description": "Maven 中央仓库"
        },
        {
          "id": "maven-aliyun",
          "name": "阿里云",
          "url": "https://maven.aliyun.com/repository/public/",
          "description": "阿里云镜像",
          "region": "CN"
        },
        {
          "id": "maven-tencent",
          "name": "腾讯云",
          "url": "https://mirrors.cloud.tencent.com/nexus/repository/maven-public/",
          "description": "腾讯云镜像",
          "region": "CN"
        },
        {
          "id": "maven-huawei",
          "name": "华为云",
          "url": "https://mirrors.huaweicloud.com/repository/maven/",
          "description": "华为云镜像",
          "region": "CN"
        },
        {
          "id": "maven-163",
          "name": "网易163",
          "url": "https://mirrors.163.com/maven/repository/maven-public/",
          "description": "网易163镜像",
          "region": "CN"
        },
        {
          "id": "maven-tsinghua",
          "name": "清华大学",
          "url": "https://mirrors.tuna.tsinghua.edu.cn/maven2/",
          "description": "清华大学 TUNA 镜像",
          "region": "CN"
        },
        {
          "id": "maven-sjtu",
          "name": "上海交大",
          "url": "https://mirrors.sjtug.sjtu.edu.cn/maven2/",
          "description": "上海交通大学 SJTUG 镜像",
          "region": "CN"
        },
        {
          "id": "maven-ustc",
          "name": "中科大",
          "url": "https://mirrors.ustc.edu.cn/maven/",
          "description": "中科大 USTC 镜像",
          "region": "CN"
        },
        {
          "id": "maven-zju",
          "name": "浙江大学",
          "url": "https://mirrors.zju.edu.cn/maven2/",
          "description": "浙江大学 ZJU 镜像",
          "region": "CN"
        },
        {
          "id": "maven-bfsu",
          "name": "北外 BFSU",
          "url": "https://mirrors.bfsu.edu.cn/maven/",
          "description": "北京外国语大学 BFSU 镜像",
          "region": "CN"
        }
      ],
      "strategy": {
        "type": "xml",
        "filePath": "~/.m2/settings.xml",
        "set": {
          "xpath": "//mirrors/mirror/url",
          "value": "{{url}}",
          "ensureStructure": {
            "createIfMissing": true,
            "createParentDirectories": true,
            "defaultStructure": "<?xml version=\"1.0\" encoding=\"UTF-8\"?>\n<settings xmlns=\"http://maven.apache.org/SETTINGS/1.0.0\"\n    xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\"\n    xsi:schemaLocation=\"http://maven.apache.org/SETTINGS/1.0.0\n    https://maven.apache.org/xsd/settings-1.0.0.xsd\">\n    <mirrors>\n        <mirror>\n            <id>mirror-switch</id>\n            <mirrorOf>*</mirrorOf>\n            <name>Mirror Switch Mirror</name>\n            <url>{{url}}</url>\n        </mirror>\n    </mirrors>\n</settings>"
          }
        },
        "get": {
          "xpath": "//mirrors/mirror/url"
        }
      },
      "backup": {
        "filePath": "~/.m2/settings.xml",
        "backupFileName": "settings.xml.backup",
        "backupOriginal": true
      },
      "metadata": {
        "supportedPlatforms": [
          "macOS",
          "linux",
          "windows"
        ],
        "supportsSpeedTest": true,
        "documentationURL": "https://maven.apache.org/"
      }
    },
    {
      "id": "brew",
      "name": "Homebrew",
      "description": "macOS 包管理器",
      "detection": {
        "command": "brew",
        "arguments": [
          "--version"
        ]
      },
      "sources": [
        {
          "id": "brew-official",
          "name": "官方源",
          "url": "https://ghcr.io/v2/homebrew/core",
          "description": "Homebrew 官方源（GitHub Containers）"
        },
        {
          "id": "brew-tuna",
          "name": "清华 TUNA",
          "url": "https://mirrors.tuna.tsinghua.edu.cn/homebrew-bottles/bottles",
          "description": "清华大学镜像",
          "region": "CN"
        },
        {
          "id": "brew-ustc",
          "name": "中科大 USTC",
          "url": "https://mirrors.ustc.edu.cn/homebrew-bottles/bottles",
          "description": "中科大镜像",
          "region": "CN"
        },
        {
          "id": "brew-sjtu",
          "name": "上海交大 SJTUG",
          "url": "https://mirrors.sjtug.sjtu.edu.cn/homebrew-bottles/bottles",
          "description": "上海交通大学镜像",
          "region": "CN"
        },
        {
          "id": "brew-zju",
          "name": "浙江大学 ZJU",
          "url": "https://mirrors.zju.edu.cn/homebrew-bottles/bottles",
          "description": "浙江大学镜像",
          "region": "CN"
        },
        {
          "id": "brew-aliyun",
          "name": "阿里云",
          "url": "https://mirrors.aliyun.com/homebrew/homebrew-bottles/bottles",
          "description": "阿里云镜像",
          "region": "CN"
        },
        {
          "id": "brew-tencent",
          "name": "腾讯云",
          "url": "https://mirrors.cloud.tencent.com/homebrew-bottles/bottles",
          "description": "腾讯云镜像",
          "region": "CN"
        },
        {
          "id": "brew-163",
          "name": "网易163",
          "url": "https://mirrors.163.com/homebrew/homebrew-bottles/bottles",
          "description": "网易163镜像",
          "region": "CN"
        }
      ],
      "strategy": {
        "type": "command",
        "set": {
          "command": "brew",
          "arguments": [
            "config",
            "set",
            "HOMEBREW_BOTTLE_DOMAIN",
            "{{url}}"
          ]
        },
        "get": {
          "command": "brew",
          "arguments": [
            "config",
            "get",
            "HOMEBREW_BOTTLE_DOMAIN"
          ],
          "outputParser": "trim"
        }
      },
      "backup": {
        "filePath": "~/.zshrc",
        "backupFileName": ".zshrc.brew.backup",
        "backupOriginal": true
      },
      "postSwitchActions": [
        {
          "type": "command",
          "command": "brew",
          "arguments": [
            "update"
          ]
        }
      ],
      "metadata": {
        "supportedPlatforms": [
          "macOS",
          "linux"
        ],
        "supportsSpeedTest": true,
        "documentationURL": "https://docs.brew.sh/"
      }
    },
    {
      "id": "orbstack",
      "name": "OrbStack",
      "description": "Docker 容器管理工具",
      "detection": {
        "customPaths": [
          "/Applications/OrbStack.app"
        ],
        "fallbackDetection": {
          "type": "file",
          "path": "~/.orbstack/config/docker.json"
        }
      },
      "sources": [
        {
          "id": "docker-official",
          "name": "官方源",
          "url": "https://registry-1.docker.io",
          "description": "Docker 官方镜像仓库"
        },
        {
          "id": "docker-aliyun",
          "name": "阿里云",
          "url": "https://registry.cn-hangzhou.aliyuncs.com",
          "description": "阿里云容器镜像服务",
          "region": "CN"
        },
        {
          "id": "docker-tencent",
          "name": "腾讯云",
          "url": "https://mirror.ccs.tencentyun.com",
          "description": "腾讯云容器镜像服务",
          "region": "CN"
        },
        {
          "id": "docker-huawei",
          "name": "华为云",
          "url": "https://swr.cn-north-4.myhuaweicloud.com",
          "description": "华为云容器镜像服务",
          "region": "CN"
        },
        {
          "id": "docker-163",
          "name": "网易163",
          "url": "https://hub-mirror.c.163.com",
          "description": "网易163镜像服务",
          "region": "CN"
        },
        {
          "id": "docker-ustc",
          "name": "中科大",
          "url": "https://docker.mirrors.ustc.edu.cn",
          "description": "中科大镜像服务",
          "region": "CN"
        },
        {
          "id": "docker-tsinghua",
          "name": "清华大学",
          "url": "https://docker.mirrors.tsinghua.edu.cn",
          "description": "清华大学 TUNA 镜像服务",
          "region": "CN"
        },
        {
          "id": "docker-sjtu",
          "name": "上海交大",
          "url": "https://docker.sjtu.edu.cn",
          "description": "上海交通大学 SJTUG 镜像服务",
          "region": "CN"
        }
      ],
      "strategy": {
        "type": "jsonpath",
        "filePath": "~/.orbstack/config/docker.json",
        "set": {
          "jsonpath": "registry-mirrors",
          "value": {
            "type": "array",
            "items": [
              {
                "type": "string",
                "value": "{{url}}"
              }
            ]
          },
          "ensureStructure": {
            "createIfMissing": true,
            "createParentDirectories": true,
            "defaultStructure": "{\"registry-mirrors\":[\"{{url}}\"]}"
          }
        },
        "get": {
          "jsonpath": "registry-mirrors.0"
        }
      },
      "backup": {
        "filePath": "~/.orbstack/config/docker.json",
        "backupFileName": "docker.json.backup",
        "backupOriginal": true
      },
      "postActions": {
        "onSourceChanged": {
          "type": "showConfirmationDialog",
          "title": "OrbStack 配置已更新",
          "message": "镜像源配置已成功修改。\n\n要使配置生效，需要重启 OrbStack Docker 引擎。\n\n是否立即重启？",
          "confirmButton": "立即重启",
          "cancelButton": "稍后重启",
          "confirmCommand": {
            "command": "/usr/local/bin/orb",
            "arguments": [
              "restart",
              "docker"
            ]
          }
        },
        "onReset": {
          "type": "showConfirmationDialog",
          "title": "OrbStack 配置已恢复",
          "message": "默认配置已成功恢复。\n\n要使配置生效，需要重启 OrbStack Docker 引擎。\n\n是否立即重启？",
          "confirmButton": "立即重启",
          "cancelButton": "稍后重启",
          "confirmCommand": {
            "command": "/usr/local/bin/orb",
            "arguments": [
              "restart",
              "docker"
            ]
          }
        }
      },
      "metadata": {
        "supportedPlatforms": [
          "macOS"
        ],
        "supportsSpeedTest": true,
        "documentationURL": "https://docs.orbstack.dev/"
      }
    },
    {
      "id": "pip3",
      "name": "Python 3 pip3",
      "description": "Python 3 包管理器 (pip3)",
      "detection": {
        "command": "pip3",
        "arguments": [
          "--version"
        ],
        "fallbackDetection": {
          "type": "file",
          "path": "~/.pip/pip.conf"
        }
      },
      "sources": [
        {
          "id": "pip3-official",
          "name": "官方源",
          "url": "https://pypi.org/simple",
          "description": "PyPI 官方源"
        },
        {
          "id": "pip3-tsinghua",
          "name": "清华大学",
          "url": "https://pypi.tuna.tsinghua.edu.cn/simple",
          "description": "清华大学 TUNA 镜像",
          "region": "CN"
        },
        {
          "id": "pip3-aliyun",
          "name": "阿里云",
          "url": "https://mirrors.aliyun.com/pypi/simple/",
          "description": "阿里云镜像",
          "region": "CN"
        },
        {
          "id": "pip3-tencent",
          "name": "腾讯云",
          "url": "https://mirrors.cloud.tencent.com/pypi/simple",
          "description": "腾讯云镜像",
          "region": "CN"
        },
        {
          "id": "pip3-huawei",
          "name": "华为云",
          "url": "https://mirrors.huaweicloud.com/repository/pypi/simple",
          "description": "华为云镜像",
          "region": "CN"
        },
        {
          "id": "pip3-douban",
          "name": "豆瓣",
          "url": "https://pypi.douban.com/simple",
          "description": "豆瓣镜像",
          "region": "CN"
        },
        {
          "id": "pip3-ustc",
          "name": "中科大",
          "url": "https://pypi.mirrors.ustc.edu.cn/simple",
          "description": "中科大 USTC 镜像",
          "region": "CN"
        },
        {
          "id": "pip3-sjtu",
          "name": "上海交大",
          "url": "https://mirrors.sjtug.sjtu.edu.cn/pypi/simple",
          "description": "上海交通大学 SJTUG 镜像",
          "region": "CN"
        },
        {
          "id": "pip3-zju",
          "name": "浙江大学",
          "url": "https://mirrors.zju.edu.cn/pypi/simple",
          "description": "浙江大学 ZJU 镜像",
          "region": "CN"
        }
      ],
      "strategy": {
        "type": "keyvalue",
        "filePath": "~/.pip/pip.conf",
        "set": {
          "key": "index-url",
          "value": "{{url}}",
          "separator": "=",
          "comment": "[global]",
          "ensureStructure": {
            "createIfMissing": true,
            "createParentDirectories": true,
            "defaultStructure": "[global]\nindex-url={{url}}\ntrusted-host={{domain}}"
          }
        },
        "get": {
          "key": "index-url",
          "separator": "="
        }
      },
      "backup": {
        "filePath": "~/.pip/pip.conf",
        "backupFileName": "pip.conf.backup",
        "backupOriginal": true
      },
      "metadata": {
        "supportedPlatforms": [
          "macOS",
          "linux",
          "windows"
        ],
        "supportsSpeedTest": true,
        "documentationURL": "https://pip.pypa.io/"
      }
    }
  ]
}
